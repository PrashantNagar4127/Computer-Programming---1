#include <stdio.h>

void main() 
{
    int arr[10] = {2, 4, 6, 8, 10}; 
    int n = 5;                      
    int newElement = 1;             
    
    if (n >= 10)
 
    for (int i = n - 1; i >= 0; i--)
     {
        arr[i + 1] = arr[i];
    }
    arr[0] = newElement;
    n++;
    
    printf("Array after inserting %d at the beginning:\n", newElement);
    for (int i = 0; i < n; i++) 
    {
        printf("%d ", arr[i]);
    }
    printf("\n");
}
