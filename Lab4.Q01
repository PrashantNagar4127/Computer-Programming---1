#include <stdio.h>

void main() 
{
    int arr[5];
    int i;    

    printf("Enter 5 integer values:\n");
    for (i = 0; i < 5; i++) 
    {
        printf("Enter value %d: ", i + 1);
        scanf("%d", &arr[i]);
    }

    printf("\nThe values you entered are:\n");
    for (i = 0; i < 5; i++)
     {
        printf("Value %d: %d\n", i + 1, arr[i]); 
    }

}